{
  "name": "minesweeper",
  "version": "1.0.1",
  "homepage": "https://github.com/HelloBugs/minesweeper",
  "authors": [
    "Victor Guo <victor.guoyu@hotmail.com>"
  ],
  "description": "Play around with Python and Pyramid",
  "main": "gulpfile.js",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
