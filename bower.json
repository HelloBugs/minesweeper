    "angular-resource": "~1.4.5",
    "angular-ui-router": "~0.2.15",
    "bootstrap": "~3.3.5",
    "font-awesome": "fontawesome#~4.4.0",
    "load-awesome": "~1.1.0",
    "underscore": "~1.8.3"
